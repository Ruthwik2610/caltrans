# Streamlit Cloud Secrets Configuration
# Copy this file to secrets.toml and fill in your values
# On Streamlit Cloud, add these in the "Secrets" section of your app settings

# OpenAI API Key (required for most features)
OPENAI_API_KEY = "sk-your-openai-api-key-here"

# Anthropic API Key (for Claude models)
ANTHROPIC_API_KEY = "sk-ant-your-anthropic-api-key-here"

# Hugging Face Token (for HF models)
HF_TOKEN = "hf_your-huggingface-token-here"

# Groq API Key (for Groq models)
GROQ_API_KEY = "gsk_your-groq-api-key-here"

# OpenAI Assistant ID for Personal Narrative Insights
CALTRANS_PERSONAL_NARRATIVE_INSIGHTS_ASSISTANT_ID = "asst_your-assistant-id-here"
